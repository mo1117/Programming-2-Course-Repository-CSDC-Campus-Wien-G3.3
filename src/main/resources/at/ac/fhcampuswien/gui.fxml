<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="576" prefWidth="1024" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.fhcampuswien.guicontroller.GUIController">
    <children>
      <Button fx:id="customNews" layoutX="436.0" layoutY="529.0" minHeight="30.0" minWidth="150.0" mnemonicParsing="false" onAction="#buttonCustomNewsClicked" text="Custom News Austria">
         <padding>
            <Insets bottom="8.0" right="8.0" top="8.0" />
         </padding></Button>
        <Button fx:id="buttonHeadlines" layoutX="13.0" layoutY="528.0" minHeight="30.0" minWidth="150.0" mnemonicParsing="false" onAction="#buttonHeadlinesAustriaClicked" text="Headlines of Austria">
            <padding>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
            </padding>
        </Button>
        <Button fx:id="buttonBitcoin" layoutX="230.0" layoutY="528.0" minHeight="30.0" minWidth="150.0" mnemonicParsing="false" onAction="#buttonBitcoinClicked" text="Bitcoin News">
            <padding>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
            </padding>
        </Button>
        <Button fx:id="buttonCount" layoutX="641.0" layoutY="527.0" minHeight="30.0" minWidth="150.0" mnemonicParsing="false" onAction="#buttonCountClicked" text="Count of Articles">
            <padding>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
            </padding>
        </Button>
        <Button fx:id="buttonQuit" layoutX="842.0" layoutY="528.0" minHeight="30.0" minWidth="150.0" mnemonicParsing="false" onAction="#buttonQuitClicked" text="Quit">
            <padding>
                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
            </padding>
        </Button>
        <VBox fx:id="vbox" alignment="TOP_CENTER" layoutX="128.0" layoutY="72.0" prefHeight="431.0" prefWidth="767.0" visible="false">
            <children>
                <Label fx:id="label" alignment="TOP_CENTER" prefHeight="432.0" prefWidth="974.0">
                    <font>
                        <Font name="Bell MT" size="20.0" />
                    </font>
                </Label>
            </children>
        </VBox>
      <Button fx:id="buttonRight" layoutX="911.0" layoutY="275.0" mnemonicParsing="false" onAction="#buttonRightClicked" text="Next" visible="false" />
      <Button fx:id="buttonLeft" layoutX="28.0" layoutY="288.0" mnemonicParsing="false" onAction="#buttonLeftClicked" text="Previous" visible="false" />
    </children>
</AnchorPane>
